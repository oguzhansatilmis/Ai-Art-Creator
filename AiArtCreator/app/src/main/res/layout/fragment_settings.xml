<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.SettingsFragment">


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline54"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.04" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:src="@drawable/vektor"
        app:layout_constraintBottom_toBottomOf="@id/guideline54"
        app:layout_constraintStart_toStartOf="@id/guideline55"
        app:layout_constraintTop_toTopOf="@id/guideline54">

    </ImageView>

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_medium"
        android:text="History"
        android:textColor="@color/black"
        android:textFontWeight="500"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="@id/guideline54"
        app:layout_constraintEnd_toEndOf="@id/guideline56"
        app:layout_constraintStart_toStartOf="@id/guideline55"
        app:layout_constraintTop_toTopOf="@id/guideline54" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.96107054" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline57"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.12" />

  <androidx.constraintlayout.widget.ConstraintLayout
      android:layout_width="wrap_content"
      app:layout_constraintStart_toStartOf="@id/guideline55"
      app:layout_constraintTop_toTopOf="@id/guideline57"
      android:layout_height="wrap_content">

      <LinearLayout
          android:layout_width="wrap_content"
          android:orientation="vertical"
          android:layout_height="wrap_content">




          <LinearLayout
              android:layout_width="match_parent"
              android:orientation="vertical"
              android:layout_height="wrap_content">
              <TextView
                  android:layout_width="match_parent"
                  android:text="Try Premium"
                  android:layout_marginLeft="20dp"
                  android:textSize="18sp"
                  android:textStyle="normal"
                  android:textColor="@color/black"
                  android:layout_height="wrap_content">

              </TextView>

              <ImageView
                  android:layout_width="match_parent"
                  android:background="@drawable/vektorasettings"
                  android:layout_height="2dp">

              </ImageView>
          </LinearLayout>


          <LinearLayout
              android:layout_width="match_parent"
              android:orientation="vertical"
              android:layout_marginTop="20dp"
              android:layout_height="wrap_content">
              <TextView
                  android:layout_width="match_parent"
                  android:text="Privacy Policy"
                  android:layout_marginLeft="20dp"
                  android:textSize="18sp"
                  android:textStyle="normal"
                  android:textColor="@color/black"
                  android:layout_height="wrap_content">

              </TextView>

              <ImageView
                  android:layout_width="match_parent"
                  android:background="@drawable/vektorasettings"
                  android:layout_height="2dp">

              </ImageView>
          </LinearLayout>
          <LinearLayout
              android:layout_width="match_parent"
              android:orientation="vertical"
              android:layout_marginTop="20dp"
              android:layout_height="wrap_content">
              <TextView
                  android:layout_width="match_parent"
                  android:text="Terms of Use"
                  android:layout_marginLeft="20dp"
                  android:textSize="18sp"
                  android:textStyle="normal"
                  android:textColor="@color/black"
                  android:layout_height="wrap_content">

              </TextView>

              <ImageView
                  android:layout_width="match_parent"
                  android:background="@drawable/vektorasettings"
                  android:layout_height="2dp">

              </ImageView>
          </LinearLayout>
          <LinearLayout
              android:layout_width="match_parent"
              android:orientation="vertical"
              android:layout_marginTop="20dp"
              android:layout_height="wrap_content">
              <TextView
                  android:layout_width="match_parent"
                  android:text="Restore Purchase"
                  android:layout_marginLeft="20dp"
                  android:textSize="18sp"
                  android:textStyle="normal"
                  android:textColor="@color/black"
                  android:layout_height="wrap_content">

              </TextView>

              <ImageView
                  android:layout_width="match_parent"
                  android:background="@drawable/vektorasettings"
                  android:layout_height="2dp">

              </ImageView>
          </LinearLayout>
          <LinearLayout
              android:layout_width="match_parent"
              android:orientation="vertical"
              android:layout_marginTop="20dp"
              android:layout_height="wrap_content">
              <TextView
                  android:layout_width="match_parent"
                  android:text="Rate Us"
                  android:layout_marginLeft="20dp"
                  android:textSize="18sp"
                  android:textStyle="normal"
                  android:textColor="@color/black"
                  android:layout_height="wrap_content">

              </TextView>

              <ImageView
                  android:layout_width="match_parent"
                  android:background="@drawable/vektorasettings"
                  android:layout_height="2dp">

              </ImageView>
          </LinearLayout>
          <LinearLayout
              android:layout_width="match_parent"
              android:orientation="vertical"
              android:layout_marginTop="20dp"
              android:layout_height="wrap_content">
              <TextView
                  android:layout_width="match_parent"
                  android:text="Send Feedback"
                  android:layout_marginLeft="20dp"
                  android:textSize="18sp"
                  android:textStyle="normal"
                  android:textColor="@color/black"
                  android:layout_height="wrap_content">

              </TextView>

              <ImageView
                  android:layout_width="match_parent"
                  android:background="@drawable/vektorasettings"
                  android:layout_height="2dp">

              </ImageView>
          </LinearLayout>
      </LinearLayout>





  </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

