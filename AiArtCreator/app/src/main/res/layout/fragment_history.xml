<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.HistoryFragment">


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.049482163" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline51"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.04136253" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline52"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.96" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.0863061" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:src="@drawable/vektor"
        app:layout_constraintStart_toStartOf="@id/guideline51"
        app:layout_constraintBottom_toBottomOf="@id/guideline46"
        app:layout_constraintTop_toTopOf="@id/guideline46"/>

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="History"
        android:textColor="@color/black"
        android:textFontWeight="500"
        android:textSize="25dp"
        android:fontFamily="@font/inter_medium"
        app:layout_constraintStart_toStartOf="@id/guideline51"
        app:layout_constraintEnd_toEndOf="@id/guideline52"
        app:layout_constraintTop_toTopOf="@id/guideline46"
        app:layout_constraintBottom_toBottomOf="@id/guideline46"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/history_recyclerView"
        app:layout_constraintTop_toTopOf="@id/guideline53"
        app:layout_constraintStart_toStartOf="@id/guideline51"
        app:layout_constraintEnd_toEndOf="@id/guideline52"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="0dp"
        android:layout_height="0dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>